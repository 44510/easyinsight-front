{
  "name": "easyinsight-web",
  "version": "0.1.0",
  "private": true,
  "description": "A mammut product based on vue cli 3 and ant-design-vue",
  "scripts": {
    "start": "yarn serve:test",
    "dev": "yarn serve:dev",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "changelog": "conventional-changelog -p custom-config-2 -i CHANGELOG.md -s -r 0",
    "commit": "npx git-cz",
    "release": "sh ./build/tag.sh",
    "serve:test": "cross-env DEPLOY_ENV=webpack PROXY_ENV=DEV PROXY_ENV_DEV=test vue-cli-service serve",
    "serve:dev": "cross-env DEPLOY_ENV=webpack PROXY_ENV=DEV PROXY_ENV_DEV=intern vue-cli-service serve",
    "serve:local": "cross-env PROXY_ENV=LOCAL vue-cli-service serve",
    "online": "cross-env PROXY_ENV=ONLINE vue-cli-service serve",
    "test": "jest",
    "build": "cross-env DEPLOY_ENV=production vue-cli-service build"
  },
  "dependencies": {
    "@antv/g6": "^4.2.0",
    "@babel/preset-typescript": "^7.16.5",
    "@emotion/jest": "^11.6.0",
    "@kvinc/util-tool": "^1.0.5",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "@vueuse/core": "^6.7.3",
    "ant-design-vue": "^2.2.6",
    "axios": "^0.19.2",
    "clipboard": "^2.0.8",
    "color": "^3.1.3",
    "core-js": "^3.6.4",
    "element-resize-detector": "^1.2.2",
    "eventemitter3": "^4.0.7",
    "jest": "^27.0.0-next.11",
    "jest-cli": "^27.4.3",
    "jest-enzyme": "^7.1.2",
    "jest-serializer-vue": "^2.0.2",
    "keyboardjs": "^2.6.4",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "qrcode": "^1.4.4",
    "quill": "^1.3.7",
    "quill-image-drop-module": "^1.0.3",
    "quill-image-resize-module": "^3.0.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.26",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "4",
    "vue-slicksort": "^1.2.0",
    "vue-virtual-scroll-list": "1.4.7",
    "vue-virtual-scroller": "^1.0.10",
    "vue3-json-viewer": "^2.2.2",
    "vuex": "^4.0.2",
    "vuex-class": "^0.3.2",
    "watermark-dom": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-syntax-jsx": "^7.16.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-router": "4.5.13",
    "@vue/cli-plugin-typescript": "4.5.13",
    "@vue/cli-plugin-unit-jest": "^5.0.0-rc.1",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "^4.5.15",
    "@vue/eslint-config-standard": "^5.1.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "babel-jest": "^27.0.0-next.11",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-import-meta": "^2.0.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.31",
    "conventional-changelog-custom-config-2": "^0.4.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "3.1.0",
    "echarts": "^5.1.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "7.17.0",
    "happypack": "^5.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.6",
    "postcss": "7.0.35",
    "postcss-cli": "^9.1.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.8.1",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "ts-jest": "^27.0.0-next.12",
    "typescript": "4.3.5",
    "unplugin-auto-import": "^0.5.11",
    "vue-cli-plugin-mock": "^0.0.10",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^15.9.7",
    "webpack-dev-middleware": "^5.0.0"
  },
  "changelog": {
    "authorName": true,
    "emojis": true,
    "commitTypes": [
      "feat",
      "fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "vue-cli-service lint",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT"
}
